{
	"manifest_version": 2,
	"name": "Download Image",
	"version": "1.1.1",

	"description": "One click image download from the context menu.",
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	  },

	"background": { 
		"scripts": ["background.js"]
	},

	"permissions": [
		"downloads",
		"contextMenus"
	]
}
